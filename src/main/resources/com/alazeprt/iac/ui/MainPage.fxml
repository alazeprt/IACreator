            <ToolBar minHeight="-Infinity" prefHeight="30.0" prefWidth="280.0" style="-fx-background-color: #cdd8e0;">
               <items>
                  <MenuButton minHeight="-Infinity" mnemonicParsing="false" prefHeight="30.0"
