            <TreeView fx:id="folderTree" prefHeight="610.0" prefWidth="200.0" style="-fx-background-color: #cdd8e0;"/>
            <AnchorPane fx:id="objectsPane" prefHeight="200.0" prefWidth="200.0"/>
         </items>
      </SplitPane>
   </children>
